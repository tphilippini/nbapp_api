{
  "name": "nbapp_api",
  "version": "0.1.0",
  "description": "Nba App Backend",
  "main": "dist/index.js",
  "scripts": {
    "lint": "babel-node ./src/scripts/lint.js",
    "test": "npm run test:unit",
    "test:unit": "jest --silent --projects ./src/tests/unit/unit.jest.json",
    "update:daily-matches": "babel-node ./src/scripts/update/daily-matches.js",
    "update:players": "babel-node ./src/scripts/update/players.js",
    "update:teams": "babel-node ./src/scripts/update/teams.js",
    "update:evals": "babel-node ./src/scripts/update/evals.js",
    "update:youtube-videos": "babel-node ./src/scripts/update/youtube-videos.js",
    "update:connect-youtube-api": "babel-node ./src/scripts/update/connect-youtube-api.js",
    "prod:update:daily-matches": "node ./dist/scripts/update/daily-matches.js",
    "prod:update:evals": "node ./dist/scripts/update/evals.js",
    "prod:update:teams": "node ./dist/scripts/update/teams.js",
    "prod:update:players": "node ./dist/scripts/update/players.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node --use_strict ./src/index.js",
    "build": "cross-env NODE_ENV=production babel ./src --out-dir ./dist",
    "start": "node ./dist/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "author": "tphilippini",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dayjs": "^1.9.1",
    "dotenv": "^8.2.0",
    "execa": "^4.0.3",
    "express": "^4.17.1",
    "express-device": "^0.4.2",
    "express-jwt": "^5.3.3",
    "google-auth-library": "^6.0.5",
    "googleapis": "^41.0.1",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.15",
    "mysql": "^2.18.1",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.4.17",
    "nodemailer-express-handlebars": "^4.0.0",
    "npm": "^6.14.7",
    "os": "^0.1.1",
    "p-iteration": "^1.1.8",
    "passport": "^0.4.1",
    "passport-facebook-token": "^3.3.0",
    "passport-google-token": "^0.1.2",
    "passport-local": "^1.0.0",
    "passport-twitter-token": "^1.3.0",
    "useragent": "^2.3.0",
    "uuid": "^3.4.0",
    "validator": "^9.4.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "cli-spinner": "^0.2.10",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.0",
    "jest": "^24.7.1",
    "jest-canvas-mock": "^2.2.0",
    "jest-extended": "^0.11.5",
    "nodemon": "^1.19.4",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0"
  }
}
