{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@@": ".",
          "@": "./src"
        },
        "compilerOptions": {
          "paths": {
            "@@": ".",
            "@": "./src"
          }
        }
      }
    ]
  ]
}
